@article{ ISI:000471092200003,
Author = {Bowley, Douglas M. and Lamb, D. and Rumbold, P. and Hunt, P. and Kayani,
   J. and Sukhera, A. M.},
Title = {{Nursing and medical contribution to Defence Healthcare Engagement:
   initial experiences of the UK Defence Medical Services}},
Journal = {{JOURNAL OF THE ROYAL ARMY MEDICAL CORPS}},
