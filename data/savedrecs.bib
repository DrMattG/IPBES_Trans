@article{ ISI:000361962900001,
Author = {Pfoestl, Eva and Kymlicka, Will},
Title = {{Minority politics in the Middle East and North Africa: the prospects for
   transformative change}},
Journal = {{ETHNIC AND RACIAL STUDIES}},
Year = {{2015}},
Volume = {{38}},
Number = {{14, SI}},
Pages = {{2489-2498}},
Month = {{NOV 14}},
Abstract = {{This special issue explores the prospects for what Jacob Mundy calls
   transformative minority politics' in the Middle East and North Africa
   (MENA) region - that is, a form of minority politics that strengthens
   democratic reform in the region, and that helps deepen a culture of
   human rights and democratic citizenship. The cases examined in the
   special issue include the Amazigh in North Africa, the Copts in Egypt,
   the Kurds in Iraq, the Palestinians in Israel, the minoritarian' regimes
   in Syria and Bahrain, and the various ethnic minorities in Iran. In this
   introduction, we try to situate this debate in a larger historical and
   international context, identifying some of the factors that might help
   support a new transformative minority politics in the region, while also
   emphasizing the factors that have inhibited it in the past, and why they
   remain powerful.}},
