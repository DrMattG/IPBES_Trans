@article{ ISI:000366436800008,
Author = {Jelinski, Murray D. and Kennedy, Richard and Campbell, John R.},
Title = {{Demographics of the Canadian cow-calf industry for the period 1991 to
   2011}},
Journal = {{CANADIAN VETERINARY JOURNAL-REVUE VETERINAIRE CANADIENNE}},
Year = {{2015}},
Volume = {{56}},
Number = {{2}},
Pages = {{163-168}},
Month = {{FEB}},
Abstract = {{The Canadian cow-calf sector is about to undergo major transformative
   change because of shifts in Canada's population demographics. To
   understand the impact of this change on the Canadian beef cow-calf
   sector, Statistics Canada census data from 1991 to 2011 were analyzed
   for trends. From 2006 to 2011, the number of Canadian cow-calf producers
   and operations decreased by 24.6\% and 26.0\%, respectively.
   Furthermore, as of 2011, 61.9\% of producers were > 50 y of age. The
   number of cow-calf producers is positioned to decrease by another 40\%
   by 2021. If Canada's cow-calf industry is to maintain its current levels
   of production then the average herd size will need to increase markedly.
   The shift towards fewer but larger operations will impact the type of
   veterinary services demanded by cow-calf producers, and the number of
   veterinarians required to service this industry. Veterinary colleges
   will need to examine whether they are producing graduates who will meet
   the changing demands of livestock producers.}},
