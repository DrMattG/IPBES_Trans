@article{ ISI:000388775100020,
Author = {Jones, Sharon A. and Michelfelder, Diane and Nair, Indira},
Title = {{Engineering managers and sustainable systems: the need for and
   challenges of using an ethical framework for transformative leadership}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2017}},
Volume = {{140}},
Number = {{1, SI}},
Pages = {{205-212}},
Month = {{JAN 1}},
Abstract = {{Despite multiple efforts, achieving sustainable societies remains an
   elusive target that will require transformative change at all levels.
   While such change has to start at the top of an organization, its
   success depends on effective implementation. For technical
   organizations, sustainability implementation rests in a large part with
   the many engineering teams responsible for meeting client needs arid
   organizational targets. Project and line managers have to influence
   those team members to adopt the moral characteristics necessary to
   engineer sustainable projects as part of everyday practice. To support
   their leadership efforts, we draw on care ethics and sustainability
   ethics in the form of an engineering ethic to propose a framework to
   orient and ground the reflective practices of those engineering
   managers. We discuss the challenges involved with internalizing this
   framework and suggest future empirical work needed before the framework
   can be operationalized. Embedding sustainability in daily engineering
   practice has to evolve at the professional level by formulating and
   propagating the relevant engineering codes, and evolve at the
   educational level by making sustainability part of the formal
   engineering curriculum. (C) 2015 Elsevier Ltd. All rights reserved.}},
