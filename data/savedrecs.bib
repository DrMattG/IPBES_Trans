@article{ ISI:000313417200001,
Author = {Isett, Kimberley R. and Glied, Sherry A. M. and Sparer, Michael S. and
   Brown, Lawrence D.},
Title = {{When Change Becomes Transformation A case study of change management in
   Medicaid offices in New York City}},
Journal = {{PUBLIC MANAGEMENT REVIEW}},
Year = {{2013}},
Volume = {{15}},
Number = {{1}},
Pages = {{1-17}},
Month = {{JAN 1}},
Abstract = {{This paper examines the implementation of large, transformative change
   in the Medicaid offices in New York City to improve efficiency and
   consumer-friendliness. A bottom-up process was engaged to design and
   implement the needed changes from those who were most affected by the
   change. Key informant interviews and observational site visits were
