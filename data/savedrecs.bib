@article{ ISI:000338408600010,
Author = {Mckeown, Mick and Dix, Julie and Jones, Fiona and Carter, Bernie and
   Malihi-Shoja, Lisa and Mallen, Ernie and Harrison, Nigel},
Title = {{Service user involvement in practitioner education: Movement politics
   and transformative change}},
