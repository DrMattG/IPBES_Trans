@article{ ISI:000431033300001,
Author = {Bailey, Sara},
Title = {{Beyond radical rhetoric: translating structural conceptions of the right
   to food into concrete demands for change}},
Journal = {{INTERNATIONAL JOURNAL OF HUMAN RIGHTS}},
Year = {{2018}},
Volume = {{22}},
Number = {{2}},
Pages = {{133-154}},
Abstract = {{Human rights work is a predominant means through which social actors
   pursue transformative change. However, the debate on whether human
   rights can accommodate claims for the eradication of the structural
   causes of oppression and deprivation has yet to be settled. In light of
   the importance of this debate to the transformative potential of human
   rights, this article sought to identify `structural conceptions' of
   human rights, selecting India's Right to Food Campaign as the case
   study. The author examined the campaign's conception of the right to
   food and found that it did understand the right to food in structural
   terms but elected not to translate the structural dimensions of its
   understanding into concrete demands for change. The author argues that
   this was because of pragmatic considerations on the one hand and a lack
   of expertise or capacity on the other. The article concludes by
   observing that the process of translating conceptions of rights into
   concrete demands for change is an important but understudied `stage' of
   the `rights-creation process', and questioning whether conceptions of
   rights developed in the context of `rights-based' work with a legalistic
   emphasis could be as far-reaching as those developed in the context of
   `rights-framed' work.}},
