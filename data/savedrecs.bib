@article{ ISI:000462197500005,
Author = {Corntassel, Jeff and Hardbarger, Tiffanie},
Title = {{Educate to perpetuate: Land-based pedagogies and community resurgence}},
Journal = {{INTERNATIONAL REVIEW OF EDUCATION}},
Year = {{2019}},
Volume = {{65}},
Number = {{1, SI}},
