@article{ ISI:000280541200011,
Author = {Marin-Flores, Oscar and Turba, Timothy and Ellefson, Caleb and Wang,
   Kang and Breit, Joe and Ahn, Jeongmin and Norton, M. Grant and Ha, Su},
